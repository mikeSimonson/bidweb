            <section id="contact" class="bg-light">
                <div class="container">
                    <div class="row text-center">
                        <div class="col-md-6 col-md-offset-3">
                            <h2><span class="text-success">{{'contact.us.anytime'|trans}}</h2>
                            <p>
                                {{'contact.us.if'|trans}}
                            </p>
                        </div>
                    </div>

                    <div class="row text-center m-t-lg">
                        <div class="col-md-4 col-md-offset-3">

                            <form class="form-horizontal" role="form" action="{{ path('bidweb_contact') }}" method="post">
                                
                                
                                <div class="form-group">
                                    <label for="name" class="col-sm-3 control-label">{{'name'|trans}}</label>

                                    <div class="col-sm-9">
                                        {{ form_row(form.userName,{'label' : false,'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="email" class="col-sm-3 control-label">{{'email'|trans}}</label>

                                    <div class="col-sm-9">
                                        {{ form_row(form.email,{'label' : false,'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="email" class="col-sm-3 control-label">{{'subject'|trans}}</label>

                                    <div class="col-sm-9">
                                        {{ form_row(form.subject,{'label' : false,'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="message" class="col-sm-3 control-label">{{'detail'|trans}}</label>

                                    <div class="col-sm-9">
                                        {{ form_row(form.message,{'label' : false,'attr': {'class': 'form-control'}}) }}
                                    </div>
