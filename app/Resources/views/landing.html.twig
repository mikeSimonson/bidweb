                <p class="pad-lft">&#0169; 2015 Bidweb</p>



            </footer>
            <!--===================================================-->
            <!-- END FOOTER -->


            <!-- SCROLL TOP BUTTON -->
            <!--===================================================-->
            <button id="scroll-top" class="btn"><i class="fa fa-chevron-up"></i></button>
            <!--===================================================-->



        </div>


        {% block javascripts %}

            {% javascripts
                '@BidwebBundle/Resources/public/nifty/plugins/pace/pace.min.js'
                '@BidwebBundle/Resources/public/nifty/js/jquery-2.1.1.min.js'
                '@BidwebBundle/Resources/public/nifty/js/bootstrap.min.js'
                '@BidwebBundle/Resources/public/nifty/plugins/fast-click/fastclick.min.js'
