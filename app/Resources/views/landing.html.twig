                                                <li {% if app.request.get('_route') == 'bidweb_payments' %} class="active-link" {% endif %}><a href="{{path('bidweb_payments')}}">{{'payments'|trans}}</a></li>

                                            </ul>
                                        </li>
                                        <!-- History menu -->
                                        <li {% if app.request.get('_route') == 'bidweb_history_worker' or 
                                                app.request.get('_route') == 'bidweb_history_payments' or  
                                                app.request.get('_route') == 'bidweb_history_project'%}  
                                                class="active-sub active" {% endif %}>
                                            <a href="#">
                                                <i class="fa fa-money"></i>
                                                <span class="menu-title">
                                                    <strong>{{'history'|trans}}</strong>
                                                </span>
                                                <i class="arrow"></i>
                                            </a>
                                            
                                            <ul {% if app.request.get('_route') == 'bidweb_history_worker' or 
                                                app.request.get('_route') == 'bidweb_history_payments' or  
                                                app.request.get('_route') == 'bidweb_history_project'%}  
                                                class="collapse in" {% else %}class="collapse" {% endif %}>
                                                
                                                <li {% if app.request.get('_route') == 'bidweb_history_worker' %} 
                                                    class="active-link" {% endif %}>
                                                    <a href="{{path('bidweb_history_worker')}}">{{'history.worker'|trans}}</a>
                                                </li>
                                                <li {% if app.request.get('_route') == 'bidweb_history_project' %} 
                                                    class="active-link" {% endif %}>
                                                    <a href="{{path('bidweb_history_project')}}">{{'history.project'|trans}}</a>
                                                </li>
                                                <li {% if app.request.get('_route') == 'bidweb_history_payments' %} 
                                                    class="active-link" {% endif %}>
                                                    <a href="{{path('bidweb_history_payments')}}">{{'history.payment'|trans}}</a>
                                                </li>

                                            </ul>
                                        </li>
                                </div>
                            </div>
                        </div>
                        <!--================================-->
                        <!--End menu-->

                    </div>
                </nav>
                <!--===================================================-->
                <!--END MAIN NAVIGATION-->

                <!--ASIDE-->
                <!--===================================================-->
                <aside id="aside-container">
                     {{ render(controller("BidwebBundle:Page:sidebar")) }}
                </aside>
                <!--===================================================-->
                <!--END ASIDE-->

            </div>



            <!-- FOOTER -->
            <!--===================================================-->
            <footer id="footer">

                <!-- Visible when footer positions are fixed -->
                <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                <div class="show-fixed pull-right">
                    <ul class="footer-list list-inline">
                        <li>
                            <p class="text-sm"><a href="{{path('freelancer')}}">{{'freelancers'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-green"></div>
                            </div>
                        </li>

                        <li>
                            <p class="text-sm"><a href="{{path('bidweb_jobs')}}">{{'jobs'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-primary"></div>
                            </div>
                        </li>
                        <li>
                            <p class="text-sm"><a href="{{path('bidweb_domain')}}">{{'domains'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-red"></div>
                            </div>
                        </li>
                        <li>
                            <p class="text-sm"><a href="{{path('bidweb_websites')}}">{{'websites'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-yellow"></div>
                            </div>
                        </li>
                        <li>
                            <p class="text-sm"><a href="{{path('bidweb_database')}}">{{'databases'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-dark"></div>
                            </div>
                        </li>
                        <li>
                            <p class="text-sm"><a href="{{path('bidweb_seotool')}}">{{'seo.tools'|trans}}</a></p>
                            <div class="progress progress-sm progress-light-base">
                                <div style="width: 100%" class="progress-bar progress-bar-pink"></div>
                            </div>
                        </li>
                        
                    </ul>
                </div>



                <!-- Visible when footer positions are static -->
